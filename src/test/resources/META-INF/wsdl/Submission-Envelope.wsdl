<wsdl:definitions
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
	xmlns:tns="http://eu.domibus.plugin/bris/wsdl/endpoint/submission/envelope/1.0"
	xmlns:dbsc="http://eu.domibus.plugin/bris/submission/components/1.0"
	targetNamespace="http://eu.domibus.plugin/bris/wsdl/endpoint/submission/envelope/1.0">

	<wsdl:types>
		<xsd:schema
			xmlns="http://eu.domibus.plugin/bris/wsdl/endpoint/submission/envelope/1.0"
			targetNamespace="http://eu.domibus.plugin/bris/wsdl/endpoint/submission/envelope/1.0"
			elementFormDefault="qualified">

			<xsd:import
				namespace="http://eu.domibus.plugin/bris/submission/components/1.0"
				schemaLocation="../xsd/submission/DB-SubmissionComponents.xsd" />
		</xsd:schema>
	</wsdl:types>

	<wsdl:message name="SubmissionEnvelope">
		<wsdl:part name="SubmissionHeader" element="dbsc:SubmissionHeader" />
		<wsdl:part name="SubmissionBody" element="dbsc:SubmissionBody" />
	</wsdl:message>

	<wsdl:message name="Acknowledgement">
		<wsdl:part name="Acknowledgement" element="dbsc:Acknowledgement" />
	</wsdl:message>

	<wsdl:message name="FaultResponse">
		<wsdl:part name="FaultResonse" element="dbsc:FaultDetail" />
	</wsdl:message>

	<wsdl:portType name="SubmissionEnvelopeInterface">
		<wsdl:operation name="submit">
			<wsdl:input message="tns:SubmissionEnvelope" />
			<wsdl:output message="tns:Acknowledgement" />
			<wsdl:fault name="FaultResponse" message="tns:FaultResponse" />
		</wsdl:operation>
	</wsdl:portType>

	<wsdl:binding name="SubmissionEnvelopeInterfaceSoapBinding"
		type="tns:SubmissionEnvelopeInterface">
		<soap:binding style="document"
			transport="http://schemas.xmlsoap.org/soap/http" />
		<wsdl:operation name="submit">
			<soap:operation soapAction="" />
			<wsdl:input>
				<soap:header message="tns:SubmissionEnvelope" part="SubmissionHeader"
					use="literal" />
				<soap:body parts="SubmissionBody" use="literal" />
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" />
			</wsdl:output>
			<wsdl:fault name="FaultResponse">
				<soap:fault name="FaultResponse" use="literal" />
			</wsdl:fault>
		</wsdl:operation>
	</wsdl:binding>

	<wsdl:service name="SubmissionEnvelopeService">
		<wsdl:port name="SubmissionEnvelopeBinding" binding="tns:SubmissionEnvelopeInterfaceSoapBinding">
			<soap:address location="/services/submission" />
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
