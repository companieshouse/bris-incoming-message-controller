<wsdl:definitions
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
	xmlns:tns="http://eu.domibus.plugin/bris/wsdl/endpoint/delivery/envelope/1.0"
	xmlns:dbdc="http://eu.domibus.plugin/bris/delivery/components/1.0"
	targetNamespace="http://eu.domibus.plugin/bris/wsdl/endpoint/delivery/envelope/1.0">

	<wsdl:types>
		<xsd:schema
			xmlns="http://eu.domibus.plugin/bris/wsdl/endpoint/delivery/envelope/1.0"
			targetNamespace="http://eu.domibus.plugin/bris/wsdl/endpoint/delivery/envelope/1.0"
			elementFormDefault="qualified">

			<xsd:import
				namespace="http://eu.domibus.plugin/bris/delivery/components/1.0"
				schemaLocation="../xsd/delivery/DB-DeliveryComponents.xsd" />
			
		</xsd:schema>
				
	</wsdl:types>

	<wsdl:message name="DeliveryEnvelope">
		<wsdl:part name="DeliveryHeader" element="dbdc:DeliveryHeader" />
		<wsdl:part name="DeliveryBody" element="dbdc:DeliveryBody" />
	</wsdl:message>

	<wsdl:message name="Acknowledgement">
		<wsdl:part name="Acknowledgement" element="dbdc:Acknowledgement" />
	</wsdl:message>

	<wsdl:message name="FaultResponse">
		<wsdl:part name="FaultResonse" element="dbdc:FaultDetail" />
	</wsdl:message>

	<wsdl:portType name="DeliveryEnvelopeInterface">
		<wsdl:operation name="submit">
			<wsdl:input message="tns:DeliveryEnvelope" />
			<wsdl:output message="tns:Acknowledgement" />
			<wsdl:fault name="FaultResponse" message="tns:FaultResponse" />
		</wsdl:operation>
	</wsdl:portType>

	<wsdl:binding name="DeliveryEnvelopeInterfaceSoapBinding"
		type="tns:DeliveryEnvelopeInterface">
		<soap:binding style="document"
			transport="http://schemas.xmlsoap.org/soap/http" />
		<wsdl:operation name="submit">
			<soap:operation soapAction="" />
			<wsdl:input>
				<soap:header message="tns:DeliveryEnvelope" part="DeliveryHeader"
					use="literal" />
				<soap:body parts="DeliveryBody" use="literal" />
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" />
			</wsdl:output>
			<wsdl:fault name="FaultResponse">
				<soap:fault name="FaultResponse" use="literal" />
			</wsdl:fault>
		</wsdl:operation>
	</wsdl:binding>

	<wsdl:service name="DeliveryEnvelopeService">
		<wsdl:port name="DeliveryEnvelopeBinding" binding="tns:DeliveryEnvelopeInterfaceSoapBinding">
			<soap:address location="/services/delivery" />
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
