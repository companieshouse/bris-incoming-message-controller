			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="udt:DateTimeType">
			<xsd:pattern value=".+\.\d{3}(Z|([+-].+))"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
