	<xsd:complexType name="DocumentFormatType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Format code of a document.
				Data. Type: CodeType
				Content:
				String
				Examples:TAR
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:DocumentTypeCodeType">
				<xsd:minLength value="1"/>
				<xsd:maxLength value="5"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="DocumentIDType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Internal document identifier
				Data. Type: IdentifierType
				Content: String
				Examples: 1420
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:IDType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="64" />
				<xsd:pattern value=".*[^\s].*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="DocumentImmediateDeliveryType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Indicator used to specify if a binary document is
				directly
				available (1) or not (0).
				Data. Type: IndicatorType
				Content:
				boolean
				Examples: 1
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:StatusAvailableIndicatorType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="DocumentPublicationDateType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Date of publication of the document
				Data. Type: DateType
				Content: Date
				Examples:2016-01-10
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="cbc:DateType" />
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="DocumentPurchasableType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Indicator that specifies if a document is public and
				therefore
				purchasable (1) or not (0).
				Data. Type: IndicatorType
				Content: boolean
				Examples: 1
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:OrderableIndicatorType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="DocumentSizeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: File size (in MB)
				Data. Type: MeasureType
				Content: decimal
				Examples: 10.00
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:ValueMeasureType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="EffectiveDateType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Date when the update became/becomes effective
				Data. Type:
				DateType
				Content: String
				Examples: 2016-01-10
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:EffectiveDateType" />
		</xsd:complexContent>
	</xsd:complexType>
