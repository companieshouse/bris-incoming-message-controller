	<xsd:complexType name="ExpiryDateType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Expiration date of a resource.
				Data. Type: DateType
				Examples: 2017-12-12
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:ExpiryDateType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CompanyItemExplanatoryLabelType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Code identifying an item. These codes are
				country-specific.
				Data. Type: CodeType
				Content: String
				Examples:AS01
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:ItemClassificationCodeType">
				<xsd:minLength value="1" />
				<xsd:pattern value=".*[^\s].*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="ExplanatoryLabelCountryType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Country of origin of the explanatory label code list
				Data. Type: CodeType
				Content: String
				Examples: BE
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:IdentificationCodeType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ExplanatoryLabelDescriptionType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Description of an item
				Data. Type: TextType
				Content:
				String
				Examples: Accounting details of a company
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:DescriptionType">
				<xsd:minLength value="1" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="ExplanatoryLabelLanguageType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Language of the label
				Data. Type: CodeType
				Content: String
				Examples: bg
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:EncodingCodeType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ExplanatoryLabelTitleType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Description of an item
				Data. Type: TextType
				Content:
				String
				Examples: Accounting sheet
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:TitleType">
				<xsd:minLength value="1" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="ManageSubscriptionCodeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Code describing the operation to be performed for a
				specific
				subscription in the ECP.
				Data. Type: CodeType
				Content: String
				Examples: Add
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:AdjustmentReasonCodeType">
				<xsd:enumeration value="ADD">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Add a company to the
							subscription list of the issuing business register
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="REMOVE">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Remove a company
							from the subscription list of the issuing business register
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="ManageSubscriptionStatusCodeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Code describing the operation to be performed for a
				specific
				subscription in the ECP.
				Data. Type: CodeType
				Content: String
				Examples: Add
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:StatusCodeType">
				<xsd:enumeration value="PROCESSED">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">The update in the
							subscription list has been successfully performed
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="IGNORED">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">The update has not
							been perform because the request was out-dated
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="ManageSubscriptionIDType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: ID a specific subscription in the ECP.
				Data. Type:
				IdentifierType
				Content: String
				Examples: 002
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:IDType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="16" />
				<xsd:pattern value=".*[^\s].*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="ParticularAmountValueType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Value of the item if the required format is an amount
				Data. Type: AmountType
				Content: Decimal
				Examples: 11,20 EUR
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:AmountType">
				<xsd:attribute name="currencyID" use="required">
					<xsd:simpleType>
						<xsd:restriction base="xsd:normalizedString">
							<xsd:enumeration value="EUR">
								<xsd:annotation>
									<xsd:documentation xml:lang="EN">Euro
									</xsd:documentation>
								</xsd:annotation>
							</xsd:enumeration>
							<xsd:enumeration value="BGN">
								<xsd:annotation>
									<xsd:documentation xml:lang="EN">Bulgarian Lev
									</xsd:documentation>
								</xsd:annotation>
							</xsd:enumeration>
							<xsd:enumeration value="CHF">
								<xsd:annotation>
									<xsd:documentation xml:lang="EN">Swiss Franc
									</xsd:documentation>
								</xsd:annotation>
							</xsd:enumeration>
							<xsd:enumeration value="CZK">
								<xsd:annotation>
									<xsd:documentation xml:lang="EN">Czech Republic
										Koruna
									</xsd:documentation>
								</xsd:annotation>
							</xsd:enumeration>
							<xsd:enumeration value="DKK">
								<xsd:annotation>
									<xsd:documentation xml:lang="EN">Danish Krone
									</xsd:documentation>
								</xsd:annotation>
							</xsd:enumeration>
							<xsd:enumeration value="GBP">
								<xsd:annotation>
									<xsd:documentation xml:lang="EN">British Pound
									</xsd:documentation>
								</xsd:annotation>
							</xsd:enumeration>
							<xsd:enumeration value="HRK">
								<xsd:annotation>
									<xsd:documentation xml:lang="EN">Croatian Kuna
									</xsd:documentation>
								</xsd:annotation>
							</xsd:enumeration>
							<xsd:enumeration value="HUF">
								<xsd:annotation>
									<xsd:documentation xml:lang="EN">Hungarian Forint
									</xsd:documentation>
								</xsd:annotation>
							</xsd:enumeration>
							<xsd:enumeration value="ISK">
								<xsd:annotation>
									<xsd:documentation xml:lang="EN">Icelandic Króna
									</xsd:documentation>
								</xsd:annotation>
							</xsd:enumeration>
							<xsd:enumeration value="NOK">
								<xsd:annotation>
									<xsd:documentation xml:lang="EN">Norwegian Krone
									</xsd:documentation>
								</xsd:annotation>
							</xsd:enumeration>
							<xsd:enumeration value="PLN">
								<xsd:annotation>
									<xsd:documentation xml:lang="EN">Polish Zloty
									</xsd:documentation>
								</xsd:annotation>
							</xsd:enumeration>
							<xsd:enumeration value="RON">
								<xsd:annotation>
									<xsd:documentation xml:lang="EN">Romanian Leu
									</xsd:documentation>
								</xsd:annotation>
							</xsd:enumeration>
							<xsd:enumeration value="SEK">
								<xsd:annotation>
									<xsd:documentation xml:lang="EN">Swedish Krona
									</xsd:documentation>
								</xsd:annotation>
							</xsd:enumeration>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="ParticularBooleanValueType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Value of the item if the required format is a boolean
				Data. Type: IndicatorType
				Content: Boolean
				Examples: 1
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:ConsolidatableIndicatorType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ParticularDateValueType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Value of the item if the required format is a date
				Data.
				Type: DateType
				Content: Date
				Examples: 2017-12-12
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:DateType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ParticularDecimalValueType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Value of the item if the required format is a decimal
				Data. Type: QuantityType
				Content: decimal
				Examples: 10,00
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:QuantityType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ParticularIntegerValueType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Value of the item if the required format is an integer
				Data. Type: QuantityType
				Content: int
				Examples: 23
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:QuantityType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ParticularStringValueType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Value of the item if the required format is a string
				Data. Type: TextType
				Content: string
				Examples: Retail
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:TextType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="2048" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="FaultErrorCodeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Code identifying an error
				Data. Type: CodeType
				Content:
				string
				Examples: PORT0001E
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:ResponseCodeType">
				<xsd:pattern value=".*[^\s].*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="FaultErrorDescriptionType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Description of an error.
				Data. Type: TextType
				Content:
				string
				Examples: Portal got a reply with no result.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:DescriptionType">
				<xsd:pattern value=".*[^\s].*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="FaultErrorSourceType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: System in error.
				Data. Type: IdentifierType
				Content:
				string
				Examples: BR-BE-01
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:ReferenceIDType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="LegalFormCodeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Code identifying a legal form of a company as defined by
				Article 1
				of Directive 2009/101/EC
				Data. Type: CodeType
				Content: String
				Examples:SA2
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cvb:LegalEntityCompanyTypeCodeType">
				<xsd:minLength value="9" />
				<xsd:pattern value=".*[^\s].*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="LegalFormCodeCountryType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Country of origin of the legal form code list
				Data. Type:
				CodeType
				Content: String
				Examples: BE
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:IdentificationCodeType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="LegalFormDescriptionType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Description of a legal form
				Data. Type: TextType
				Content:
				String
				Examples: Private Limited Company registered in Register of
				Business
				Enterprises. Share capital (minimum): 50.000 euros.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:DescriptionType">
				<xsd:pattern value=".*[^\s].*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="LegalFormLanguageType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Language of the legal form description
				Data. Type:
				CodeType
				Content: String
				Examples: bg
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:EncodingCodeType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="LegalFormCodeValueType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Name of the legal form
				Data. Type: NameType
				Content:
				String
				Examples: Privata Aktiebolag
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cvb:LegalEntityLegalNameType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="LegislationReferenceType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Relevant national or EU legislation.
				Data. Type: TextType
				Content: String
				Examples: Article 13 of Directive 2005/56/EC, as
				amended by Directive
				2012/17/EU (Art. 2)
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:LegalReferenceType" />
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="MergerType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Code of a merger as defined by Article 2(2) of Directive
				2005/56/EC
				[5].
				Data. Type: CodeType
				Content: String
				Examples:
				Acquisition
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:NotificationTypeCodeType">
				<xsd:enumeration value="ACQUISITION">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Cross-border merger
							by acquisition
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="FORMATION_OF_NEW_COMPANY">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Cross-border merger
							by formation of new company
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="OWNED_COMPANY">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Cross-border merger
							of wholly owned company
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="MessageIDType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Reference of the message
				Data. Type: IDType
				Content:
				String
				Examples: normalizedString
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:ReferenceIDType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="64" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="NumberOfResultsInMessageType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Number of results in message.
				Data. Type: QuantityType
				Content: int
				Examples: 4
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:DeliveredQuantityType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="NumberOfResultsInRegistersType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Total number of results found in register(s).
				Data. Type:
				QuantityType
				Content: int
				Examples: 5
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:DeliveredQuantityType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="PaymentReferenceType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Reference of the payment for one or several documents
				belonging to
				a specific Business Register.
				Data. Type: TextType
				Content: String
				Examples: 31234567
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:ReferenceType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="64" />
				<xsd:pattern value=".*[^\s].*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="PostalCodeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Postal code
				Data. Type: TextType
				Content: String
				Examples:
				4000
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:PostalZoneType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="10" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="ProceedingType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Code of a branch disclosure event as given in Article 5a
				(1) of
				Directive 89/666/EEC [6].
				Data. Type: CodeType
				Content: String
				Examples: WindingUpOpening
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:NotificationTypeCodeType">
				<xsd:enumeration value="WINDING_UP_OPENING">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Winding-up Opening
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="WINDING_UP_TERMINATION">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Winding-up
							Termination
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="WINDING_UP_OPENING_AND_TERMINATION">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Winding-up Opening
							and Termination
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="WINDING_UP_REVOCATION">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Winding-up
							Revocation
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="INSOLVENCY_OPENING">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Insolvency Opening
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="INSOLVENCY_TERMINATION">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Insolvency
							Termination
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="INSOLVENCY_OPENING_AND_TERMINATION">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Insolvency Opening
							and Termination
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="INSOLVENCY_REVOCATION">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Insolvency
							Revocation
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="STRIKING_OFF">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Striking-off
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="NO_STRIKING_OFF_OF_BRANCH">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Article 5a(5) – No-Striking-off-of Branch
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="RelevanceType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Value indicating how much a result matches the search
				criteria.
				Data. Type: NumericType
				Content: int
				Examples: 2
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:WeightNumericType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ProcessingNodeNameIDType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Specifies the name of the node where the
				BR-PingRequest has been processed.
				Data.
				Type: IdentifierType
				Content: String
				Examples: XXXX111
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:IDType">
				<xsd:minLength value="1" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="ProcessingTimeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Specifies a time duration in milliseconds
				Examples: 600001
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:TimeAmountType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="SubscriptionStatusCodeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Code describing the status for a specific subscription
				in the ECP.
				Data. Type: CodeType
				Content: String
				Examples: Subscribed
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:StatusCodeType">
				<xsd:enumeration value="SUBSCRIBED">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">The business
							register is in the subscription list for a specific parent
							company
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="NOT_SUBSCRIBED">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">The business
							register is not in the subscription list for a specific parent
							company
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="TestCaseIDType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Test Case identifier
				Data. Type:IdentifierType
				Examples: TC-FN-009-08
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:IDType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="TestConditionIDType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Test execution condition identifier
				Data.
				Type:IdentifierType
				Examples:1
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:IDType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="TestExecutionIDType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Test Execution identifier.
				Represents that actual sending of a single message
				in the context of a test
				session
				Data. Type:IdentifierType
				Examples:
				123456
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:ReferenceIDType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="TestPackageIDType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Test package identifier
				Data. Type:IdentifierType
				Examples:TPK-FN-BDN
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:IDType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="TestSessionIDType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Test Session identifier.
				unique identifier that correlates a specific test case
				with a group of test cases.
				Data. Type:IdentifierType
				Examples:
				123456
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:ReferenceIDType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="TimeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Time when the message was sent.To specify a time zone,
				enter a time
				in UTC time by adding a "Z" behind the time
				Data. Type:
				TimeType
				Examples: 12:00:00Z
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:EffectiveTimeType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="TimeAmountType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Specifies a time duration in milliseconds
				Examples: 600001
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:TimeAmountType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="UpdateLEDCodeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Code used to define the type of update to be performed
				in the LED.
				Data. Type: CodeType
				Content: String
				Examples: Update
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:AdjustmentReasonCodeType">
				<xsd:enumeration value="UPDATE">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Update of company
							data
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="DELETE">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Delete of company
							data
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="UpdateLEDUpdateIDType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: ID a specific subscription in the ECP.
				Data. Type:
				IdentifierType
				Content: String
				Examples: 002
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:IDType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="16" />
				<xsd:pattern value=".*[^\s].*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="UpdateLEDStatusCodeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Status of a LED request to indicate if it has been
				processed or
				not.
				Data. Type: TextType
				Content: String
				Examples: Update
				done on Company Name: IKEA
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:StatusCodeType">
				<xsd:enumeration value="PROCESSED">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">The update has been
							processed
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="IGNORED">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">The update has not
							been processed.
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="URIType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Unique Resource Identifier of the external reference of
				the
				document
				Data. Type: IdentifierType
				Content: string
				Examples:
				https://www.iso.org/
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:URIType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="2048" />
				<xsd:pattern value="https?://.+" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="WarningType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Description of an exception returned by SOLR if there is
				an error
				in processing a request for a simple search.
				Data. Type:
				CodeType
				Content: string
				Examples: WARN_ES_01
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:ResponseCodeType">
				<xsd:pattern value=".*[^\s].*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:simpleType name="LanguageCodeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Code identifying a language
				Data. Type: CodeType
				Content:
				String
				Examples:SA2
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:language">
			<xsd:enumeration value="bg">
