	<xsd:complexType name="DataExportFileURIType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Indicator for the data export file sent
				over FTP from the business register within the context of
				full update request.
				Data. Type: URIType
				Examples:
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:URIType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="128" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="DateType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Date when the message was sent.
				Data. Type:DateType
				Examples: 2017-12-12
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:DateType"/>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="DateTimeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Date when the message was sent.
				Data. Type:DateType
