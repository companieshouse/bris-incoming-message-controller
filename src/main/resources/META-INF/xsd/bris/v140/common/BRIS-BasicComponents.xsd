	<xsd:complexType name="AckIndicatorType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Indicator for technical ack
				Data. Type: IndicatorType
				Examples: 1 (accepted)
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:AcceptedIndicatorType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="AddressLine1Type">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Registered address of the company
				Data. Type: TextType
				Content: String
				Examples: 12, Bvd. de Pigalle, 75001 Paris, France
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:LineType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="255" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="AddressLine2Type">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Registered address of the company
				Data. Type: TextType
				Content: String
				Examples: 12, Bvd. de Pigalle, 75001 Paris, France
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:LineType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="255" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="AddressLine3Type">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Registered address of the company
				Data. Type: TextType
				Content: String
				Examples: 12, Bvd. de Pigalle, 75001 Paris, France
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:LineType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="255" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="AttachmentReferenceType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Indicator for technical ack
				Data. Type: IndicatorType
				Examples: 1 (accepted)
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:ReferenceIDType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="64" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="BusinessRegisterIDType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Element making it possible to identify the domestic
				register of
				origin of a company
				Data. Type: IdentifierType
				Content:
				String
				Examples: RMC
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:RegistrationIDType">
				<xsd:minLength value="2" />
				<xsd:maxLength value="15" />
				<xsd:pattern value=".*[^\s].*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="BusinessRegisterNameType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Name of the business register where a company is
				registered
				Data. Type: NameType
				Content: String
				Examples: Registro
				Mercantil Central
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:NameType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="128" />
				<xsd:pattern value=".*[^\s].*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="CityType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Name of the city of the registered office
				Data. Type:
				TextType
				Content: String
				Examples: Paris
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:CityNameType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="255" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="CompanyAlternateIDType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Alternate identifier for a company. Typically, alternate
				IDs are
				used by some Member States participating in pilots.
				Data.
				Type: IdentifierType
				Content: String
				Examples: XXXX111
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:CompanyIDType">
				<xsd:minLength value="1" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="CompanyEUIDType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: The European Unique Identifier is a unique identifier of
				a company
				for the purpose of communication between registers in the
				context of
				BRIS.
				Data. Type: IdentifierType
				Content: String
				Examples:
				FRIG.2010012341-Z
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:CompanyIDType">
				<xsd:pattern value="[A-Z]{2}[^ ]{2,15}\..{3,35}" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="CompanyItemValidityStartDateType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Applicability start date of an item
				Data. Type: DateType
				Content: Date
				Examples:2016-01-10
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="cbc:DateType" />
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="CompanyItemValidityEndDateType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Applicability end date of an item
				Data. Type: DateType
				Content: Date
				Examples:2016-01-10
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="cbc:DateType" />
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="CompanyLegalFormType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Legal form of the company
				Data. Type: CodeType
				Content:
				String
				Examples: Societe par action simplifiee , S.A.S
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cvb:LegalEntityCompanyTypeCodeType">
				<xsd:pattern value=".*[^\s].*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="CompanyNameType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Name of the company
				Data. Type: NameType
				Content: String
				Examples: GDF Suez
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cvb:InputNameType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="1000" />
				<xsd:pattern value=".*[^\s].*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="CompanyNameLanguageSpecificType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Name of the company
				Data. Type: NameType
				Content: String
				Examples: GDF Suez
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cvb:LegalEntityLegalNameType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="1000" />
				<xsd:pattern value=".*[^\s].*" />
				<xsd:attribute name="languageID" type="LanguageCodeType"
					use="required" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="CompanyRegistrationNumberType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Company number refers to the registration number of the
				company in
				the domestic register of origin
				Data. Type: IdentifierType
				Content: String
				Examples:ATU99999999
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cvb:LegalEntityLegalIDType">
				<xsd:minLength value="3" />
				<xsd:maxLength value="35" />
				<xsd:pattern value=".*[^\s].*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="CorrelationIDType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Reference of the message
				Data. Type: IDType
				Content:
				String
				Examples: normalizedString
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:ReferenceIDType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="64" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
